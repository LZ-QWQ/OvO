        <Button x:Name="Button6" Content="6" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="212,126,0,0" Click="Button6_Click"/>
        <Button x:Name="Button7" Content="7" VerticalAlignment="Top" Margin="0,164,442,0" Click="Button7_Click"/>
